<div id="UserStatus">
    <ul class="list-group"></ul>
    <% for(let index = 0; index < usersStatuses.length; index++) { %>
        <li class="list-group-item"><a id="<%=usersStatuses[index]._id %>" href="/dms/<%= usersStatuses[index].username %>" class="<%= (usersStatuses[index].status) == 'Online' ? 'text-success' : 'text-secondary' %>"> <%= usersStatuses[index].username %> </a></li>
    <% } %>
    </ul>
</div>